        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>function pad2(n){ return (n&lt;10 ? "0"+n : ""+n); }

var t = time != null ? time.trim() : "";
var p = t.split(":");
var hh = parseInt(p[0],10) || 0;
var mm = parseInt(p[1],10) || 0;
var ss = p.length > 2 ? parseInt(p[2],10) || 0 : 0;

time_norm = pad2(hh) + ":" + pad2(mm) + ":" + pad2(ss);
hour = hh;
minute = mm;
second = ss;</jsScript_script>
